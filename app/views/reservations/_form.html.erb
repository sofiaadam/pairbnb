<%= form_for [@listing, @reservation] do |form| %>
<%= form.hidden_field :listing_id, value: @listing.id %>  
  
    <div>
    <%= form.label :start_date %>
    <%= form.date_field :start_date%>
    </div>
    <div>
    <%= form.label :end_date %>
    <%= form.date_field :end_date%>
    </div>
    <%= form.submit "Submit Reservation" %>
    <%end%>